  "homepage": "https://github.com/zowe/zowe-cli/tree/master/packages/imperative#readme",
  "bugs": {
    "url": "https://github.com/zowe/zowe-cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zowe/zowe-cli.git"
  },
  "keywords": [
    "CLI",
    "framework",
    "zowe"
